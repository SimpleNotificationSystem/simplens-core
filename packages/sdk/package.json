{
    "name": "@simplens/sdk",
    "version": "1.0.2",
    "description": "SDK for building SimpleNS notification provider plugins",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "default": "./dist/index.js"
        },
        "./*": {
            "types": "./dist/*.d.ts",
            "import": "./dist/*.js",
            "default": "./dist/*.js"
        }
    },
    "scripts": {
        "build": "tsc",
        "prepublishOnly": "npm run build"
    },
    "keywords": [
        "simplens",
        "notification",
        "plugin",
        "sdk"
    ],
    "author": "SimpleNS Team",
    "license": "MIT",
    "dependencies": {
        "zod": "^3.22.0"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "typescript": "^5.0.0"
    },
    "files": [
        "dist",
        "README.md"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/SimpleNotificationSystem/simplens-core"
    }
}